{
    "name": "duncanmcclean/guest-entries",
    "description": "Perform CRUD operations in the front-end of your site.",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "DuncanMcClean\\GuestEntries\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DuncanMcClean\\GuestEntries\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Guest Entries",
            "description": "Perform CRUD operations in the front-end of your site."
        },
        "laravel": {
            "providers": [
                "DuncanMcClean\\GuestEntries\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "statamic/cms": "dev-master"
    },
    "require-dev": {
        "pestphp/pest": "^2.2 || ^3.0",
        "orchestra/testbench": "^8.28 || ^9.6.1 || ^10.0",
        "spatie/test-time": "^1.2",
        "laravel/pint": "^1.18"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pixelfear/composer-dist-plugin": true,
            "pestphp/pest-plugin": true
        }
    }
}
